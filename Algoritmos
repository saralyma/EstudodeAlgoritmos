var
   x , y , z , k , resultado : inteiro
   
inicio
// Seção de Comandos
x <- 2
y <- 4
z <- 9
k <- 3
resultado <-0
se ( x < y + k) E ( k * 2 > 4) E ( z+ k < 12 ) entao
   resultado <-1
  senao
    se ( y + 10 < z ) ou ( z > 12) entao
    resultado <- 2
      senao
      se  ( x + 2 ) >= ( y - 4 ) entao
          resultado <-3
          senao
          resultado <-4
          fimse
       fimse
fimse

 
fimalgoritmo
